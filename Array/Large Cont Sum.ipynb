   "source": [
    "def large_cont_sum(arr):\n",
    "    cur_sum = max_sum = arr[0]\n",
    "    for ele in arr[1:]:\n",
    "        cur_sum = max(cur_sum + ele, ele)\n",
    "        max_sum = max(cur_sum, max_sum)\n",
    "    return max_sum"
   ]
  },
